{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.6.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "guzzlehttp/guzzle": "^6.3",
        "aloha/twilio": "*",
        "phpoffice/phpword": "^0.14.0",
        "dompdf/dompdf": "^0.8.2",
        "snowfire/beautymail": "dev-master",
        "cyber-duck/laravel-excel": "^1.2",
        "laravel/socialite": "3.*",
        "laravel/cashier": "~7.0",
        "sander-van-hooft/laravel-invoicable": "dev-master",
        "exodusanto/administrator": " *",
        "spatie/laravel-permission": "*",
        "cartalyst/stripe-laravel": "*",
        "google/apiclient": "^2.0",
        "laravelcollective/html": "^5.4",
        "shivella/laravel-bitly": "^1.0",
        "thiagoalessio/tesseract_ocr": "^2.2",
        "laravel/scout": "^4.0",
        "algolia/algoliasearch-client-php": "^1.25",
        "jeroennoten/laravel-adminlte": "^1.23",
        "creativeorange/gravatar": "~1.0",
        "pusher/pusher-php-server": "^3.0",
        "sebastian-berc/repositories": "*",
        "jrean/laravel-user-verification": "dev-master",
        "dacastro4/laravel-gmail": "^0.4.1",
        "laravel/passport": "^6.0",
        "cmgmyr/messenger": "^2.15",
        "webpatser/laravel-uuid": "^3.0",
        "consoletvs/charts": "^6.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "talvbansal/media-manager": "^2.5",
        "maennchen/zipstream-php": "v0.4.1",
        "spatie/laravel-medialibrary": "7.0.0",
        "intervention/imagecache": "^2.3",
        "intervention/image": "^2.4",
        "unisharp/laravel-filemanager": "~1.8",
        "mercuryseries/flashy": "^1.0",
        "kris/laravel-form-builder": "^1.14",
        "khill/lavacharts": "^3.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "nahid/talk": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "*",  
        "barryvdh/laravel-ide-helper": "^2.4",
        "squizlabs/php_codesniffer": "^3.3"    
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "App\\Providers\\EventServiceProvider::postAutoloadDump"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
